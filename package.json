{
  "name": "db2p",
  "version": "1.0.0",
  "description": "A simple package for giving distance between two gps points",
  "scripts": {
    "test": "vitest run",
    "build": "tsc",
    "ci": "npm run build && npm run check-format && npm run check-exports && npm run test",
    "format": "prettier --write .",
    "check-format": "prettier --check .",
    "check-exports": "attw --pack . --ignore-rules=cjs-resolves-to-esm",
    "dev": "vitest",
    "local-release": "changeset version && changeset publish",
    "prepublishOnly": "npm run ci"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/gvarner13/db2p.git"
  },
  "keywords": [
    "typescript",
    "gps"
  ],
  "author": "Gary Varner <me@garyvarner.me> (https://garyvarner.me)",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/gvarner13/db2p/issues"
  },
  "homepage": "https://github.com/gvarner13/db2p#readme",
  "files": [
    "dist"
  ],
  "type": "module",
  "devDependencies": {
    "@arethetypeswrong/cli": "^0.16.4",
    "@changesets/cli": "^2.27.9",
    "prettier": "^3.3.3",
    "typescript": "^5.6.3",
    "vitest": "^2.1.4"
  },
  "main": "dist/index.js"
}
